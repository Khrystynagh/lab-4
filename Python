n = int(input("Введіть натуральне число n:"))       #Введення натурального числа

a = 1.5
c = b = 0

if n >= 4:                                          #Перевірка n
    for i in range(4, n+1, 1):                      #Знаходження vN за допомогою арифметичного цикла
        vN = a * (i + 1) / (i ** 2 + 1) - b * c
        c = b
        b = a
        a = vN
        print(vN)                                   #Виведення vN
else:
    print("n виходить за межі інтервалу")
